<!DOCTYPE html>
<html>
  <head>
    <title>Rangertrak</title>
    <!-- Bootstrap -->
    <link rel="stylesheet" href="/bootstrap.min.css" type="text/css">

  </head>
  <body>
    <script src="http://code.jquery.com/jquery-latest.js"></script>
    <script src="js/bootstrap.min.js"></script>
    


    <div class="container">

      <div class="navbar navbar-inverse">
        <div class="navbar-inner">
          <a class="brand" href="#">Rangertrak</a>
          <ul class="nav">
            <li class="active"><a href="index">Main</a></li>
            <li><a href="setup">Setup</a></li>
            <li><a href="reports">Reports</a></li>
          </ul>
          <form class="navbar-search pull-right">
            <input type="text" class="search-query" placeholder="Search">
          </form>
        </div>
      </div>

      <div class="row">
        
        <div class="span3">
          <!--Left content-->
          <form>
            <legend>Create Incident</legend>
            <input type="text" placeholder="Street Name 1">
            <input type="text" placeholder="Street Name 2">
            <input type="text" placeholder="Camp Name">
  
            <label>Incident Type</label>
            <select>
              <option>Medical</option>
              <option>Police</option>
              <option>Fire</option>
              <option>Mental Health</option>
              <option>Vehicle</option>
            </select>

            <label>Priority</label>
            <select>
              <option>Urgent</option>
              <option>High</option>
              <option>Normal</option>
              <option>Low</option>
            </select>

            <textarea rows="5" placeholder="Notes"></textarea>

            <span class="help-block">Enter incident details and hit Submit.</span>
            <label class="checkbox">
              <input type="checkbox"> Shift log only
            </label>
            <button type="submit" class="btn">Submit</button>
          </form>
        </div>

        <div class="span6">
          <!--Body content-->
          <div class="row">
            <div class="span2">

              <button type="button" class="btn btn-danger" data-toggle="collapse" data-target="#demo1">
                Medical 2324 - <br>8:15 | I Street
              </button>
              <div id="demo1" class="collapse in"> Dehydrated female, 20 yrs old </div>

              <button type="button" class="btn btn-warning" data-toggle="collapse" data-target="#demo2">
                Fire 2319 - <br>4:15 | Espl
              </button>
              <div id="demo2" class="collapse in"> Burn Barrel spilling over </div>

              <button type="button" class="btn btn-success" data-toggle="collapse" data-target="#demo3">
                Assist 2336 - <br>2:30 | A Street
              </button>
              <div id="demo3" class="collapse in"> Disoriented male, 45 yrs old </div>

            </div>
            
            <div class="span2">

              <button type="button" class="btn btn-success" data-toggle="collapse" data-target="#demo4">
                Vehicle 2290 - <br>7:00 | C Street
              </button>
 
              <div id="demo4" class="collapse in"> Vehicle blocking fire lane </div>


            </div>
            
            <div class="span2">

              <button Type="button" class="btn btn-success" data-toggle="collapse" data-target="#demo5">
                Mntl Hlth 2294 - <br>6:00 | J Street
              </button>
 
              <div id="demo5" class="collapse in"> Very upset female, 30 yrs old </div>


            </div>
          </div>
        </div>
        
        <div class="span3">
          <!--Right content-->
          <form>
            <legend>Resources</legend>
            <label>Type</label>
            <select>
              <option>OOD</option>
              <option>Shift Lead</option>
              <option>GD Long</option>
              <option>LEAL</option>
              <option>Ranger</option>
            </select>
            <input type="text" placeholder="Enter Name">
            <button type="submit" class="btn">Submit</button>
          </form>

          <table class="table table-hover"> 

            <tbody>
              <tr><td>OOD - Splinter</td></tr>
              <tr><td>Shift Lead - Easy E</td></tr>
              <tr><td>Shift Lead - Bob</td></tr>
              <tr><td>Shift Lead - App</td></tr>
              <tr><td>GD Long - Bayou</td></tr>
              <tr><td>LEAL - Mickey</td></tr>
            </tbody>

          </table>

        </div>
      
      </div>
    </div>

  </body>
</html>